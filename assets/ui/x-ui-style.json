{
   "panel":{
      "margins":{
         "top":1,
         "left":1,
         "bottom":2,
         "right":2
      },
      "borderStyle": "plain",
      "borderWidth": 2,
      "borderRadius": 5,
      "borderColor": "0x000000",
      "fillColor": "0xf0d0b0",
      "alpha": 1
   },
   "overlay":{
     "fillColor": "0x0",
     "alpha": 0.5
   },
   "button":{
      "borderStyle":"raised",
      "borderWidth":2,
      "borderRadius":3,
      "shadowColor": "0x000000",
      "fillColor": "0xe86010",
      "highlightColor": "0xf0d0b0",
      "alpha":1
   },
   "hotkey":{
      "margins":{
         "left":2
      },
      "color":"#000000",
      "align":"left"
   },
   "label":{
      "margins":{
         "left":2,
         "top":2
      },
      "color":"#e86010",
      "font":"22px Arial Black",
      "align":"center",
      "shadow":
      {
         "offsetX":2,
         "offsetY":2,
         "fill":true
      }
   },
   "marker":{
      "margins":{
         "top":-1,
         "left":-1,
         "bottom":-2,
         "right":-2
      },
      "width":32,
      "height":32,
      "borderWidth":2,
      "borderColor":{
         "normal" : "0xffffff",
         "warning" : "0xff4f78"
      }
   }
}
