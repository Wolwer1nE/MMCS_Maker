{
   "panel":
   {
      "borderStyle": "plain",
      "borderWidth": 2,
      "borderRadius": 5,
      "borderColor": "0x0",
      "fillColor": "0xf0d0b0",
      "alpha": 1
   },
   "overlay":
   {
     "fillColor": "0x0",
     "alpha": 0.5
   },
   "button":
   {
      "borderStyle":"raised",
      "borderWidth":2,
      "borderRadius":3,
      "shadowColor": "0x0",
      "fillColor": "0xe86010",
      "highlightColor": "0xf0d0b0",
      "alpha":1
   },
   "hotkey":
   {
      "color":"#000000",
      "align":"left"
   },
   "label":
   {
      "render":"bitmap",
      "color":"#e86010",
      "fontFamily":"mancraftia",
      "fontSize":"22px",
      "align":"center",
      "shadow":
      {
         "offsetX":2,
         "offsetY":2,
         "fill":true
      }
   },
   "marker":{
      "width":32,
      "height":32,
      "borderWidth":2,
      "borderColor":{
         "normal" : "0xffffff",
         "warning" : "0xff4f78"
      }
   },
   "assets":{
     "fonts":["mancraftia"]
   }
}
